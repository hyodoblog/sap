{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "dist",
    "paths": {
      "~/*": [
        "./src/*"
      ],
      "@/*": [
        "./src/*"
      ]
    },
    "types": [
      "@nuxt/types",
      "@nuxtjs/axios",
      "@nuxtjs/moment",
      "@types/node",
      "vuetify",
    ]
  },
  "exclude": [
    "node_modules",
    ".nuxt"
  ],
  "include": [
    "src"
  ]
}
